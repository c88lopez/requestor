{
  "Login": {
    "Url": "/path/to/auth",
    "Fields": {
      "Username": ["usernameFieldName", "username"],
      "Password": ["passwordFieldName", "password"]
    }
  },

  "Report": "Reports.csv",
  "Results": "Results.csv",

  "Days": [4],
  "Timeout": 900,

  "SkipNoDays": true,

  "Domain": "domain.com",
  "WithCore": false,

  "Tokens": {
    "DateFrom": "~@DateFrom@~",
    "DateTo": "~@DateTo@~"
  },

  "Workers": 1,
  "Limit": -1
}
